import java.util.Scanner;

public class p1 {
    String a;
    public p1()
    {
        System.out.println("enter message");
        Scanner input=new Scanner(System.in);
        a=input.nextLine();
    }
    public static void main(String args[])
    {
        p1 obj=new p1();
        System.out.println("message is "+obj.a);
    }
}
